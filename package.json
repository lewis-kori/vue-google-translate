{
  "name": "vue-google-translate",
  "version": "0.0.6",
  "description": "A vue.js wrapper for the google translate API. The package easens localization of vue.js powered web apps.",
  "author": {
    "name": "Lewis Kori",
    "email": "lewiskosh@gmail.com"
  },
  "repository": "https://github.com/lewis-kori/vue-google-translate.git",
  "bugs": "https://github.com/lewis-kori/vue-google-translate.git/issues",
  "keywords": [
    "vue",
    "google translate",
    "localization"
  ],
  "license": "MIT",
  "main": "dist/vue-google-translate.ssr.js",
  "browser": "dist/vue-google-translate.esm.js",
  "module": "dist/vue-google-translate.esm.js",
  "unpkg": "dist/vue-google-translate.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "vue-google-translate": "^0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
